-- Scripts:

local function LGQV_fake_script() -- Frame.Hotkey Opener 
	local script = Instance.new('LocalScript', Frame)

	local frame = script.Parent.Parent.Frame --Change Frame to your Frame name!
	local hotkey = Enum.KeyCode.RightShift -- Change Q to your own hotkey!
	
	local UIS = game:GetService("UserInputService")
	local open = true
	
	
	
	UIS.InputBegan:Connect(function(key, gp)
		if key.KeyCode == hotkey then
			if UIS:GetFocusedTextBox() == nil then
				if open == false then
					open = true 
					frame.Visible = open
				elseif open == true then
					open = false
					frame.Visible = open
				end
			end
		end
	end)
	
end
coroutine.wrap(LGQV_fake_script)()
local function IZLC_fake_script() -- Frame.Drag Script 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService("UserInputService")
	function dragify(Frame)
	    dragToggle = nil
	    local dragSpeed = 1.0
	    dragInput = nil
	    dragStart = nil
	    local dragPos = nil
	    function updateInput(input)
	        local Delta = input.Position - dragStart
	        local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + Delta.X, startPos.Y.Scale, startPos.Y.Offset + Delta.Y)
	        game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.25), {Position = Position}):Play()
	    end
	    Frame.InputBegan:Connect(function(input)
	        if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
	            dragToggle = true
	            dragStart = input.Position
	            startPos = Frame.Position
	            input.Changed:Connect(function()
	                if input.UserInputState == Enum.UserInputState.End then
	                    dragToggle = false
	                end
	            end)
	        end
	    end)
	    Frame.InputChanged:Connect(function(input)
	        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
	            dragInput = input
	        end
	    end)
	    game:GetService("UserInputService").InputChanged:Connect(function(input)
	        if input == dragInput and dragToggle then
	            updateInput(input)
	        end
	    end)
	end
	
	dragify(script.Parent)
	
	
end
coroutine.wrap(IZLC_fake_script)()
local function DJMNLAI_fake_script() -- Frame.UnPacked Loading Script 
	local script = Instance.new('LocalScript', Frame)

	pcall(
		function()
			game.StarterGui:SetCore(
				"ChatMakeSystemMessage",
				{
					Text = "{Oxipine#6857} UnPacked is loading.. Not Done BTW",
					Color = Color3.new(0, 0, 0)
				}
			)
		end
	)
	
	wait (5)
	
	pcall(
		function()
			game.StarterGui:SetCore(
				"ChatMakeSystemMessage",
				{
					Text = "{Oxipine#6857} Unpacked loaded succesfully, join our discord for some thing bout the script",
					Color = Color3.new(0, 0, 0)
				}
			)
		end
	)
	
	wait (3)
	
	pcall(
		function()
			game.StarterGui:SetCore(
				"ChatMakeSystemMessage",
				{
					Text = "{Unpacked} Press 'Rshift' to open and close the gui :D",
					Color = Color3.new(0, 0, 0)
				}
			)
		end
	)
	
	wait (120)
	
	pcall(
		function()
			game.StarterGui:SetCore(
				"ChatMakeSystemMessage",
				{
					Text = "Soup How Ya Doing?",
					Color = Color3.new(0, 1, 0)
				}
			)
		end
	)
end
coroutine.wrap(DJMNLAI_fake_script)()
local function ZTHNNNE_fake_script() -- Frame.Pos Script 
	local script = Instance.new('LocalScript', Frame)

	
	script.Parent:TweenPosition(UDim2.new(0.1, 0, 0.1, 0), "Out", "Quad", 1)
	script.Parent.Draggable = true
end
coroutine.wrap(ZTHNNNE_fake_script)()
local function OYVTNW_fake_script() -- Frame.Auto Ex Script 
	local script = Instance.new('LocalScript', Frame)

	local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
	while wait() do
		for i,v in pairs (UI.CircleAction.Specs) do
			v.Duration = 0;
			v.Timed = true;
		end
	end
end
coroutine.wrap(OYVTNW_fake_script)()
local function FORMV_fake_script() -- nil.Inf Jetpack Fuel 
	local script = Instance.new('LocalScript', nil)

	for _, v in next, getgc(true) do
		if (type(v) == 'table' and rawget(v, 'LocalMaxFuel')) then
			v.LocalMaxFuel = math.huge
			v.LocalFuel = math.huge
			v.LocalFuelType = 'Rocket'
		end
	end
end
coroutine.wrap(FORMV_fake_script)()
local function AUPZOGX_fake_script() -- nil.inf nitro 
	local script = Instance.new('LocalScript', nil)

	local mouse = game:GetService('Players').LocalPlayer:GetMouse()
	
	local function hotkeyHandler(key)
		if (key == 'q') then
			for _, v in next, getgc(true) do
				if (type(v) == 'table' and rawget(v, 'Nitro')) then
					v.Nitro = 250
				end
			end
		end
	end
	
	mouse.KeyDown:connect(hotkeyHandler)
end
coroutine.wrap(AUPZOGX_fake_script)()
