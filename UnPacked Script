local function GOZC_fake_script() -- nil.No Wait 
	local script = Instance.new('LocalScript', nil)

	local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
	while wait() do
		for i,v in pairs (UI.CircleAction.Specs) do
			v.Duration = 0;
			v.Timed = true;
		end
	end
end
coroutine.wrap(GOZC_fake_script)()
local function EGCJP_fake_script() -- nil.Fireworks 
	local script = Instance.new('LocalScript', nil)

	for a,b in pairs(getgc()) do 
		if typeof(b) == "function" then 
			for c,d in pairs(debug.getupvalues(b)) do 
				if typeof(d) == "table" and rawget(d,"Fireworks") then
					d.Fireworks(math.huge)
				end
			end
		end
	end
end
coroutine.wrap(EGCJP_fake_script)()
local function JTALANS_fake_script() -- nil.CTRL DELTE 
	local script = Instance.new('LocalScript', nil)

	local Plr = game:GetService("Players").LocalPlayer
	local Mouse = Plr:GetMouse()
	
	Mouse.Button1Down:connect(function()
		if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
		if not Mouse.Target then return end
		Mouse.Target:Destroy()
	end)
end
coroutine.wrap(JTALANS_fake_script)()
local function OJYNLL_fake_script() -- nil.No Doors 
	local script = Instance.new('LocalScript', nil)

	game:GetService('Workspace').Doors:ClearAllChildren()
	
end
coroutine.wrap(OJYNLL_fake_script)()
local function KWKS_fake_script() -- nil.Gun Mods 
	local script = Instance.new('LocalScript', nil)

	local GunsToMod = {'Pistol', 'AK47', 'Uzi', 'Revolver', 'Sniper', 'Shotgun'};
	for i,v in next, game:GetService('ReplicatedStorage').Game.ItemConfig:GetChildren() do
		for i2, v2 in next, GunsToMod do
			if v2 == v.Name then
				rawset(require(v), 'MagSize', math.huge);
				rawset(require(v), 'ReloadTime', 0);
				rawset(require(v), 'FireFreq', math.huge);
				rawset(require(v), 'FireAuto', true);
				rawset(require(v), 'CamShakeMagnitude', 0);
			end;
		end;
	end;
end
coroutine.wrap(KWKS_fake_script)()
local function WPNM_fake_script() -- nil.Get Guns 
	local script = Instance.new('LocalScript', nil)

	while true do 
		wait (30)
		for i,v in pairs(workspace.Givers:GetDescendants()) do
			if v:IsA("ClickDetector") then
				fireclickdetector(v)
			end
		end
	end
end
coroutine.wrap(WPNM_fake_script)()
