-- Gui to Lua
-- Version: 3.2

-- Instances:

local Hacker = Instance.new("ScreenGui")

--Properties:

Hacker.Name = "Hacker"
Hacker.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Hacker.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Scripts:

local function NFRT_fake_script() -- Hacker.God Mode 
	local script = Instance.new('LocalScript', Hacker)

	local Client = {}
	
	for i,v in pairs(debug.getregistry()) do
		if typeof(v) == "function" then
			for i2,v2 in pairs(debug.getupvalues(v)) do
				if typeof(v2) == "table" and v2.FireServer then
					Client.FireServerT = v2
				end
			end
		end    
	end    
	
	
	game:GetService('RunService').RenderStepped:connect(function()
		wait(0.1)
		if game.Workspace[game.Players.LocalPlayer.Name].Humanoid.Health < 100 then
			Client.FireServerT:FireServer(unpack({
				[1] = "dd96918e",
				[2] = "Donut"
			}))
			wait(0.1)  
			Client.FireServerT:FireServer(unpack({
				[1] = "f205949f"
			}))
		end
	end)
end
coroutine.wrap(NFRT_fake_script)()
local function VCOJMLE_fake_script() -- Hacker.Gun Mods 
	local script = Instance.new('LocalScript', Hacker)

	local GunsToMod = {'Pistol', 'AK47', 'Uzi', 'Revolver', 'Sniper', 'Shotgun, Taser'};
	for i,v in next, game:GetService('ReplicatedStorage').Game.ItemConfig:GetChildren() do
		for i2, v2 in next, GunsToMod do
			if v2 == v.Name then
				rawset(require(v), 'MagSize', math.huge);
				rawset(require(v), 'ReloadTime', 0);
				rawset(require(v), 'FireFreq', math.huge);
				rawset(require(v), 'FireAuto', true);
				rawset(require(v), 'CamShakeMagnitude', 0);
			end;
		end;
	end;
end
coroutine.wrap(VCOJMLE_fake_script)()
local function MHSGKN_fake_script() -- Hacker.CTRL Destroy 
	local script = Instance.new('LocalScript', Hacker)

	local Plr = game:GetService("Players").LocalPlayer
	local Mouse = Plr:GetMouse()
	
	Mouse.Button1Down:connect(function()
		if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
		if not Mouse.Target then return end
		Mouse.Target:Destroy()
	end)
end
coroutine.wrap(MHSGKN_fake_script)()
local function CPQOCGY_fake_script() -- Hacker.No Wait 
	local script = Instance.new('LocalScript', Hacker)

	local UI = require(game:GetService("ReplicatedStorage").Module:WaitForChild("UI"));
	while wait() do
		for i,v in pairs (UI.CircleAction.Specs) do
			v.Duration = 0;
			v.Timed = true;
		end
	end
end
coroutine.wrap(CPQOCGY_fake_script)()
local function RGAZII_fake_script() -- Hacker.Inf Nitro Decreser = false 
	local script = Instance.new('LocalScript', Hacker)

	for a,b in pairs(getreg()) do 
		if typeof(b) == "function" then 
			for c,d in pairs(debug.getupvalues(b)) do 
				if typeof(d) == "table" and d.FireServer then
					local FS = d.FireServer
					d.FireServer = function(a1, ...)
						if typeof(({...})[2]) == "number" and game:GetService("Players").LocalPlayer.Character:FindFirstChild("InVehicle") then 
							return FS(a1, ({...})[1], -math.huge)
						else
							return FS(a1, ...)
						end
					end
				end
			end
		end
	end
end
coroutine.wrap(RGAZII_fake_script)()
local function LYJNY_fake_script() -- Hacker.no doors 
	local script = Instance.new('LocalScript', Hacker)

	game:GetService('Workspace').Doors:ClearAllChildren()
end
coroutine.wrap(LYJNY_fake_script)()
local function ATUMTL_fake_script() -- Hacker.Inf Jetpack Fuel 
	local script = Instance.new('LocalScript', Hacker)

	for _, v in next, getgc(true) do
		if (type(v) == 'table' and rawget(v, 'LocalMaxFuel')) then
			v.LocalMaxFuel = math.huge
			v.LocalFuel = math.huge
			v.LocalFuelType = 'Rocket'
		end
	end
end
coroutine.wrap(ATUMTL_fake_script)()
local function NKHTJ_fake_script() -- Hacker.Inf Jump 
	local script = Instance.new('LocalScript', Hacker)

	local InfiniteJumpEnabled = true
	game:GetService("UserInputService").JumpRequest:connect(function()
		if InfiniteJumpEnabled then
			game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
		end
	end)
	local InfiniteJump = CreateButton("Infinite Jump: Off", StuffFrame)
	InfiniteJump.Position = UDim2.new(0,10,0,130)
	InfiniteJump.Size = UDim2.new(0,150,0,30)
	InfiniteJump.MouseButton1Click:connect(function()
		local state = InfiniteJump.Text:sub(string.len("Infinite Jump: ") + 1) --too lazy to count lol
		local new = state == "Off" and "On" or state == "On" and "Off"
		InfiniteJumpEnabled = new == "On"
		InfiniteJump.Text = "Infinite Jump: " .. new
	end)
end
coroutine.wrap(NKHTJ_fake_script)()
local function DIPOIQ_fake_script() -- Hacker.INf Nitro 
	local script = Instance.new('LocalScript', Hacker)

	local mouse = game:GetService('Players').LocalPlayer:GetMouse()
	
	local function hotkeyHandler(key)
		if (key == 'q') then
			for _, v in next, getgc(true) do
				if (type(v) == 'table' and rawget(v, 'Nitro')) then
					v.Nitro = 250
				end
			end
		end
	end
	
	mouse.KeyDown:connect(hotkeyHandler)
end
coroutine.wrap(DIPOIQ_fake_script)()
local function AHXLRTU_fake_script() -- Hacker.WEE 
	local script = Instance.new('LocalScript', Hacker)

	while true do 
		wait (30)
		for i,v in pairs(workspace.Givers:GetDescendants()) do
			if v:IsA("ClickDetector") then
				fireclickdetector(v)
			end
		end
	end
end
coroutine.wrap(AHXLRTU_fake_script)()
