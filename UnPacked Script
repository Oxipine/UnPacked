-- Gui to Lua
-- Version: 3.2

-- Instances:

local AllMiscInShit = Instance.new("ScreenGui")
local Script = Instance.new("Folder")

--Properties:

AllMiscInShit.Name = "All Misc In Shit"
AllMiscInShit.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
AllMiscInShit.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Script.Name = "Script"
Script.Parent = AllMiscInShit

-- Scripts:

local function OWDDY_fake_script() -- Script.Unpacked Local Scripts 
	local script = Instance.new('LocalScript', Script)

	require(game:GetService("ReplicatedStorage").Game.Notification).new(
	{Text = "Thank You For Using UnPacked!", Duration = 5
	})
end
coroutine.wrap(OWDDY_fake_script)()
local function EBSLQ_fake_script() -- nil.CTRL Delete 
	local script = Instance.new('LocalScript', nil)

	local Plr = game:GetService("Players").LocalPlayer
	local Mouse = Plr:GetMouse()
	
	Mouse.Button1Down:connect(function()
		if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftControl) then return end
		if not Mouse.Target then return end
		Mouse.Target:Destroy()
	end)
end
coroutine.wrap(EBSLQ_fake_script)()
local function MMMYPVP_fake_script() -- nil.Inf Nitro 
	local script = Instance.new('LocalScript', nil)

	local mouse = game:GetService('Players').LocalPlayer:GetMouse()
	
	local function hotkeyHandler(key)
		if (key == 'q') then
			for _, v in next, getgc(true) do
				if (type(v) == 'table' and rawget(v, 'Nitro')) then
					v.Nitro = 250
				end
			end
		end
	end
	
	mouse.KeyDown:connect(hotkeyHandler)
end
coroutine.wrap(MMMYPVP_fake_script)()
local function HNKG_fake_script() -- nil.Decrease False 
	local script = Instance.new('LocalScript', nil)

	for a,b in pairs(getreg()) do 
		if typeof(b) == "function" then 
			for c,d in pairs(debug.getupvalues(b)) do 
				if typeof(d) == "table" and d.FireServer then
					local FS = d.FireServer
					d.FireServer = function(a1, ...)
						if typeof(({...})[2]) == "number" and game:GetService("Players").LocalPlayer.Character:FindFirstChild("InVehicle") then 
							return FS(a1, ({...})[1], -math.huge)
						else
							return FS(a1, ...)
						end
					end
				end
			end
		end
	end
end
coroutine.wrap(HNKG_fake_script)()
local function CRTGF_fake_script() -- nil.God Mode 
	local script = Instance.new('LocalScript', nil)

	local Client = {}
	
	for i,v in pairs(debug.getregistry()) do
		if typeof(v) == "function" then
			for i2,v2 in pairs(debug.getupvalues(v)) do
				if typeof(v2) == "table" and v2.FireServer then
					Client.FireServerT = v2
				end
			end
		end    
	end    
	
	
	game:GetService('RunService').RenderStepped:connect(function()
		wait(0.1)
		if game.Workspace[game.Players.LocalPlayer.Name].Humanoid.Health < 100 then
			Client.FireServerT:FireServer(unpack({
				[1] = "dd96918e",
				[2] = "Donut"
			}))
			wait(0.1)  
			Client.FireServerT:FireServer(unpack({
				[1] = "f205949f"
			}))
		end
	end)
end
coroutine.wrap(CRTGF_fake_script)()
local function GDKLJVH_fake_script() -- nil.Gun Mods 
	local script = Instance.new('LocalScript', nil)

	local GunsToMod = {'Pistol', 'AK47', 'Uzi', 'Revolver', 'Sniper', 'Shotgun', 'Taser'};
	for i,v in next, game:GetService('ReplicatedStorage').Game.ItemConfig:GetChildren() do
		for i2, v2 in next, GunsToMod do
			if v2 == v.Name then
				rawset(require(v), 'MagSize', math.huge);
				rawset(require(v), 'ReloadTime', 0);
				rawset(require(v), 'FireFreq', math.huge);
				rawset(require(v), 'FireAuto', true);
				rawset(require(v), 'CamShakeMagnitude', 0);
			end;
		end;
	end;
end
coroutine.wrap(GDKLJVH_fake_script)()
local function HUWQZVY_fake_script() -- nil.No Doors 
	local script = Instance.new('LocalScript', nil)

	for i,v in pairs(workspace:GetDescendants()) do
		if v.Name == "SwingDoor" then
			v:Destroy()
		end
	end
end
coroutine.wrap(HUWQZVY_fake_script)()
